    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.marvelappapresentation.view.activitys.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerMemberFragment"
            app:destination="@id/registerMemberFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerMemberFragment"
        android:name="com.example.marvelappapresentation.view.RegisterMemberFragment"
        android:label="fragment_membership"
        tools:layout="@layout/fragment_membership" />
